server {
  listen 80;
  server_name 54.208.193.221;

  root /home/ec2-user/beike-admin/dist;
  index index.html;

  location / {
    try_files $uri $uri/ /index.html;
  }

  location /css/ {
    expires 1y;
    add_header Cache-Control "public";
  }

  location /js/ {
    expires 1y;
    add_header Cache-Control "public";
  }

  location ~* \.(jpg|jpeg|png|gif|ico|svg)$ {
    expires 1y;
    add_header Cache-Control "public";
  }

  error_page  500 502 503 504  /50x.html;
}
